    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ch.ethz.inf.vs.partone.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ch.ethz.inf.vs.partone.SensorActivity2"
            android:label="@string/title_activity_sensor2"
            android:parentActivityName="ch.ethz.inf.vs.partone.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ch.ethz.inf.vs.partone.MainActivity" />
        </activity>
        <activity
            android:name="ch.ethz.inf.vs.partone.ActuatorsActivity"
            android:label="@string/title_activity_actuators"
            android:parentActivityName="ch.ethz.inf.vs.partone.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ch.ethz.inf.vs.partone.MainActivity" />
        </activity>
    </application>

</manifest>