<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ch.ethz.inf.vs.a1.fabianu.sensors" >
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ch.ethz.inf.vs.a1.fabianu.sensors.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ch.ethz.inf.vs.a1.fabianu.sensors.SensorActivity2"
            android:label="@string/title_activity_sensor2"
            android:parentActivityName="ch.ethz.inf.vs.a1.fabianu.sensors.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ch.ethz.inf.vs.a1.fabianu.sensors.MainActivity" />
        </activity>
        <activity
            android:name="ch.ethz.inf.vs.a1.fabianu.sensors.ActuatorsActivity"
            android:label="@string/title_activity_actuators"
            android:parentActivityName="ch.ethz.inf.vs.a1.fabianu.sensors.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ch.ethz.inf.vs.a1.fabianu.sensors.MainActivity" />
        </activity>
    </application>

</manifest>
